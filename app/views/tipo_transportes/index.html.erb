<p id="notice"><%= notice %></p>

<h1>Listing Tipo Transportes</h1>

<table>
  <thead>
    <tr>
      <th>Cod tiptrans</th>
      <th>Integer</th>
      <th>Nom tiptrans</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_transportes.each do |tipo_transporte| %>
      <tr>
        <td><%= tipo_transporte.cod_tiptrans %></td>
        <td><%= tipo_transporte.integer %></td>
        <td><%= tipo_transporte.nom_tiptrans %></td>
        <td><%= link_to 'Show', tipo_transporte %></td>
        <td><%= link_to 'Edit', edit_tipo_transporte_path(tipo_transporte) %></td>
        <td><%= link_to 'Destroy', tipo_transporte, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo transporte', new_tipo_transporte_path %>
