<p id="notice"><%= notice %></p>

<h1>Listing Documentos</h1>

<table>
  <thead>
    <tr>
      <th>Cod tipdoc</th>
      <th>Cod pedido</th>
      <th>Nom documento</th>
      <th>Ruta archivo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @documentos.each do |documento| %>
      <tr>
        <td><%= documento.cod_tipdoc %></td>
        <td><%= documento.cod_pedido %></td>
        <td><%= documento.nom_documento %></td>
        <td><%= documento.ruta_archivo %></td>
        <td><%= link_to 'Show', documento %></td>
        <td><%= link_to 'Edit', edit_documento_path(documento) %></td>
        <td><%= link_to 'Destroy', documento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Documento', new_documento_path %>
